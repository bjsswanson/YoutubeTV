<nav class="navbar navbar-custom">
    <div class="container">
        <div class="navbar-header">
            <!--<img src="images/logo.png"/>-->
        </div>
    </div>
</nav>
<div class="container-fluid">
    <div class="row">
        <div class="col-md-5 col-sm-5">
			<div class="youtube">
				<form class="form-horizontal">
                    <div class="form-group">
						<div class="input-group-lg">
							<input type="text" class="form-control" id="youtubeLink" placeholder="Youtube Link">
						</div>
					</div>
                    <div class="form-group">
						<div class="btn-group btn-group-justified" role="group">
							<div class="btn-group" role="group">
								<button id="addNext" class="btn btn-primary btn-lg">Add Next</button>
							</div>
							<div class="btn-group" role="group">
								<button id="addLast" class="btn btn-primary btn-lg">Add Last</button>
							</div>
						</div>
					</div>
				</form>
            </div>
        </div>
        <div class="col-md-6 col-md-offset-1 col-sm-6 col-sm-offset-1">
			<ul id="videos" class="list-group">
				{{#each playing}}
                    <li id="{{this.id}}" class="list-group-item">
						<img src="{{this.thumnails.default}}"/>
						<span>{{this.title}}</span>
					</li>
				{{/each}}
			</ul>
        </div>
    </div>
</div>